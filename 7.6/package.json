{
  "dependencies": {
    "cypress": "^14.5.0"
  },

  "scripts": {
    
    "cy:desktop:headed":   "cypress open --config-file cypress.config.js --browser chrome",
    "cy:desktop:headless": "cypress run  --config-file cypress.config.js --browser chrome",

   
    "cy:mobile:headed":    "cypress open --config-file cypress.config.mobile.js --browser chrome",
    "cy:mobile:headless":  "cypress run  --config-file cypress.config.mobile.js --browser chrome"
  }
}